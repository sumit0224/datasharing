# Production environment variables for AirForShare

NODE_ENV=production
PORT=3000

# Redis Configuration
REDIS_URL=redis://localhost:6379

# Client URL (production domain)
CLIENT_URL=https://airforshare.com

# Logging
LOG_LEVEL=info

# Security (generate secure keys for production)
SESSION_SECRET=change-this-to-a-random-secure-string-in-production

# File Upload Limits
MAX_FILE_SIZE=104857600
